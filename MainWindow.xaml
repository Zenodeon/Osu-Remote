<Window x:Class="OsuRemote.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OsuRemote"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800"
        Closing="OnClosingWindow">
    <Grid>
        <Button Content="Start Listening" HorizontalAlignment="Left" Margin="50,41,0,0" VerticalAlignment="Top" Height="31" Width="102" Click="Button_Click_1"/>
        <Button Content="Stop Listening" HorizontalAlignment="Left" Margin="50,82,0,0" VerticalAlignment="Top" Height="31" Width="102" Click="Button_Click"/>
        <Button Content="Send UDP" HorizontalAlignment="Left" Margin="50,118,0,0" VerticalAlignment="Top" Height="31" Width="102" Click="Button_Click_2"/>
        <Button Content="Simulate" HorizontalAlignment="Left" Margin="50,154,0,0" VerticalAlignment="Top" Height="31" Width="102" Click="SKey"/>
        <TextBox HorizontalAlignment="Left" Height="159" Margin="304,185,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="377"/>

    </Grid>
</Window>
